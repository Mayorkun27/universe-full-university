{
     "version": 2,
     "builds": [
       {
         "src": "api/api.js",
         "use": "@vercel/node"
       },
       {
         "src": "public/**/*",
         "use": "@vercel/static"
       }
     ],
     "routes": [
       {
         "src": "/api/(.*)",
         "dest": "/api/api.js"
       },
       {
         "src": "/(.*)",
         "dest": "/public/$1"
       }
     ]
   }   